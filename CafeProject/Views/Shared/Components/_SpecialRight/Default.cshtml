@model List<Menu>
<div class="col-lg-6">
    <div class="row">
        <div class="right-list">
            <div class="col-lg-12">
                @foreach (var item in Model)
                {
                    <div class="tab-item">
                        <img src="@item.İmageUrl" alt="">
                        <h4>@item.Name</h4>
                        <p>@item.Description</p>
                        <div class="price">
                            <h6>@item.Price</h6>
                        </div>
                    </div>
                }
              
            </div>
        
        </div>
    </div>
</div>